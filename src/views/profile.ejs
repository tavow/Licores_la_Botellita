<!DOCTYPE html>
<html lang="es">

<head>
	<%- include('partials/head', { title: "Perfil Usuario" }); %>
</head>

<body>

	<header>
		<%- include('./partials/header') %>
	</header>

	<!-- Container -->
	<div class="container" style="margin-top: 40px;">
		<div class="row">
			<div class="col-md-9">
				<h2>Hi <%= usuario.nombre %>
						<%= usuario.apellido %>
				</h2>
				<img src="/img/<%= usuario.img %> ">
				<br>
				<div class="alert alert-success">
					<%= usuario.correo %>
				</div>
				<br>
			</div>
		</div>
	</div>
	<!-- //Container -->

	<footer class="footer-distributed">
		<%- include('partials/footer'); %>
	</footer>
</body>

</html>